# Salin ke .env lalu isi. Dipakai oleh: podman-compose / podman compose
# cp .env.podman.example .env

# Database (container db)
DB_PASSWORD=changeme
DB_ROOT_PASSWORD=rootsecret

# Admin API
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_admin_password

# JWT (min 32 karakter)
JWT_SECRET=minimal_32_character_secret_for_jwt_signing

# CORS: origin frontend (kosongkan jika akses same-origin lewat nginx proxy)
ALLOW_ORIGIN=
